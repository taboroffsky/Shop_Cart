
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <link href="~/Content/MyStyleSheet.css" rel="stylesheet" />
    <script src="~/Scripts/jquery-1.10.2.min.js"></script>
    <script src="~/Scripts/jquery.validate.min.js"></script>
    <script src="~/Scripts/jquery.unobtrusive-ajax.min.js"></script>
    <title>_Default</title>
</head>
<body>
    <div class="div-container">
        <div class="div-header">
            Shopping Online - Get Reward)
        </div><!--div-header-->
        <div class="div-topmenu">
            <ul>
                <li>                    
                @Html.ActionLink("Home","Index","Home")@*LinkText,ActionName,ControllerName*@
                </li>
                <li>
                    @Html.ActionLink("News", "Index", "News")
                </li>
                <li>
                    @Html.ActionLink("Products", "Index", "Products")
                </li>
                <li>
                    @Html.ActionLink("Contuct Us", "Index", "ContactUs")
                </li>
                <li>
                    @Html.ActionLink("Manage Color", "Index", "Colors")
                </li>
                <li>
                    @Html.ActionLink("News Management", "Index", "NewsManagment")
                </li>
                <li>
                    @Html.ActionLink("Cart", "Index", "ShoppingCart")
                </li>
            </ul>
        </div><!--div-topmenu-->
        <div class="clear"></div>
        <div class="div-left">
            <div class="category-title">Category</div><!--category-title-->
            <div class="category-list">
               @Html.Action("CategoryPartial","Category")
            </div><!--category-list-->
        </div><!--div-left-->
        <div class="div-main" id="div-main">
           
            
                @RenderBody()
            

        </div><!--div-main-->
        <div class="clear"></div>
        <div class="div-footer">
            <p>Copyright) All rights reserved</p>
            <p>Email : dr@email.em</p>
        </div><!--div-footer-->
    </div><!--div-container-->
</body>
</html>
